@model InrappSos.AstridWeb.Models.SendCodeViewModel
@{
    ViewBag.Title = "Skicka";
}

<div class="row padding-top-55">
    <h2>@ViewBag.Title</h2>
</div>

@using (Html.BeginForm("SendCode", "Account", new { ReturnUrl = Model.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    @Html.Hidden("rememberMe", @Model.RememberMe)
    <h4>Skicka verifieringskod</h4>
    <hr />
    <div class="row">
        <div class="col-md-8">
            Välj Two-Factor Authentication Provider:
            @Html.DropDownListFor(model => model.SelectedProvider, Model.Providers)
            <input type="submit" value="Spara" class="btn btn-default" />
        </div>
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
